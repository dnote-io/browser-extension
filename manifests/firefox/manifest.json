{
  "name": "Dnote",
  "version": "0.1.0",
  "description": "Capture your coding knowledge without leaving the browser.",
  "applications": {
    "gecko": {
      "id": "addon@example.com",
      "strict_min_version": "42.0"
    }
  },
  "icons": {
    "16": "images/iconx16.png",
    "48": "images/iconx48.png",
    "128": "images/iconx128.png"
  },
  "manifest_version": 2,
  "browser_action": {
    "default_icon": {
      "16": "images/iconx16.png",
      "32": "images/iconx32.png"
    },
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["scripts/background.js"]
  },
  "content_scripts": [],
  "permissions": ["storage", "*://127.0.0.1:5000/*", "*://api.dnote.io/*"]
}
